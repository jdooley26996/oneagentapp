<table border="1">
<tr>
 <th>ID</th>
 <th>Title</th>
 <th>Amount</th>
 </tr>
 <% @expenses.each do |expense| %>
 	<tr>
 	 <td> <%= expense.id %> </td>
 	 <td> <%= expense.title %> </td>
 	 <td> <%= expense.transaction_date %> </td>
 	 <td> <%=number_to_currency  expense.amount %> </td>
 	 </tr>
 	<% end %>
 	</table> 
