<%- model_class = OpenHouse -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:Name) %>:</strong></dt>
  <dd><%= @open_house.Name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Phone_number) %>:</strong></dt>
  <dd><%= @open_house.Phone_number %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Email_address) %>:</strong></dt>
  <dd><%= @open_house.Email_address %></dd>
  <dt><strong><%= model_class.human_attribute_name(:How_did_you_here_about_our_open_house) %>:</strong></dt>
  <dd><%= @open_house.How_did_you_here_about_our_open_house %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Are_you_working_with_a_Realtor) %>:</strong></dt>
  <dd><%= @open_house.Are_you_working_with_a_Realtor %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Realtors_name_and_contact_information) %>:</strong></dt>
  <dd><%= @open_house.Realtors_name_and_contact_information %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Would_you_like_to_be_emailed_new_properties) %>:</strong></dt>
  <dd><%= @open_house.Would_you_like_to_be_emailed_new_properties %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @open_house.user_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              open_houses_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_open_house_path(@open_house), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              open_house_path(@open_house),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
